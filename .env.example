# =====================================================
# SPL REGISTRATION ENVIRONMENT VARIABLES
# =====================================================
# Variables required for team registration and updates
# DO NOT MODIFY THESE VARIABLE NAMES

SPL_API_KEY=your_spl_api_key_here


# =====================================================
# LEARNING CONTENT ENVIRONMENT VARIABLES
# =====================================================
# Variables used in weekly learning notebooks
# You may add additional variables as needed

# API Authentication and Configuration
# ------------------------------------
# Your API Key for service authentication (never commit the real key)
API_KEY=your_secret_key_here

# The base URL for your API service
API_URL=https://api.example.com/v1

# Optional API Configuration
# -------------------------
# Timeout for API requests in seconds
API_TIMEOUT=30

# Debug mode (true/false)
DEBUG=False

# Organization ID (for services like OpenAI)
ORGANIZATION_ID=your_org_id_here

# Additional service-specific variables
# ------------------------------------
# Rate limiting settings
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=60

# Proxy configuration (if needed)
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=https://proxy.example.com:8080
